memory {{ memory }} MB

molecule {
  {{ charge }} {{ multiplicity }}
  {{ molecule }}
}

set {
  basis sto-3g
  {{#has_nopenshells}}
  reference rhf
  {{/has_nopenshells}}
}

{{ multipoles }}

o, wfn = {{ task }}("scf", return_wfn = True)
fchk(wfn, "{{ prefix }}.fchk" )
{{#doHessian}}
np.array(o)
{{/doHessian}}
